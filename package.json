{
  "name": "turno-api",
  "version": "1.0.0",
  "description": "Turno Management API Server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:json": "cross-env NODE_ENV=development USE_JSON_PROXY=true PORT=3001 CORS_ORIGIN=http://localhost:5173 tsx watch src/index.ts",
    "start:json": "cross-env NODE_ENV=development USE_JSON_PROXY=true PORT=3001 CORS_ORIGIN=http://localhost:5173 tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src/**/*.ts",
    "db:setup": "node scripts/setup-cosmosdb.js",
    "db:migrate": "node scripts/migrate-data.js",
    "db:status": "node scripts/cosmos-utils.js status",
    "db:stats": "node scripts/cosmos-utils.js stats",
    "db:backup": "node scripts/cosmos-utils.js backup",
    "db:utils": "node scripts/cosmos-utils.js",
    "db:wizard": "node scripts/setup-wizard.js"
  },
  "dependencies": {
    "@azure/cosmos": "^4.0.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "zod": "^3.22.4",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1",
    "xlsx": "^0.18.5",
    "@js-temporal/polyfill": "^0.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "cross-env": "^7.0.3",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3",
    "vitest": "^1.0.4",
    "eslint": "^8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0"
  }
}
